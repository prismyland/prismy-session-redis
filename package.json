  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "prettier --check src/**/*.ts examples/*/src/**/*.ts",
    "format": "prettier --write src/**/*.ts examples/*/src/**/*.ts",
    "test": "ava --verbose",
    "test-ci": "nyc ava && nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "codecov",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.6.6",
    "@types/redis": "^2.8.13",
    "ava": "^2.2.0",
    "codecov": "^3.5.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prismy": "^0.9.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "prismy-session": "0.0.1-0"
  },
  "dependencies": {
    "prismy-session": "0.0.1-1",
    "redis": "^2.8.0"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "src/specs/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  }
